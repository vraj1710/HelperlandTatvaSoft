    <script type="text/javascript">
        $('#tab1').DataTable({
            "aaSorting": [],
            columnDefs: [{
                orderable: false,
                targets: [1, 3]
            }]
        });
        $('#tab2').DataTable({
            "aaSorting": [],
            columnDefs: [{
                orderable: false,
                targets: [1, 3, 5]
            }]
        });
        $('#tab3').DataTable({
            "aaSorting": [],
            columnDefs: [{
                orderable: false,
                targets: [4]
            }]
        });
        $('#tab4').DataTable({
            "aaSorting": [],
            columnDefs: [{
                orderable: false,
                targets: [1, 2]
            }]
        });

        $(document).on("click",".shispopup", function () {
            var x = $(this).attr("z-id");
            console.log(x);

             $.ajax({
                url: '@Url.Action("_upcomingshpopup", "home")',
                type: 'get',
                dataType: 'html',
                contentType: 'application/html',
                data: {id:x},


                success: function (resp) {
                    $("#exampleModal .modal-body").html(resp);
                    $("#exampleModal").modal('show');
                },
                error: function (err) {
                    alert("Error while inserting data");
                }

            });
        });


    </script>

    <script>
        $(function () {
            $("#exportBtn").click(function (e) {
                console.log("hii");
                var table = $("#serviceHistory");
                console.log(table);
                if (table && table.length) {
                    $(table).table2excel({
                        exclude: ".noExl",
                        name: "Excel Document Name",
                        filename: "ServiceHistory" + new Date().toISOString().replace(/[\-\:\.]/g, "") + ".xls",
                        fileext: ".xls",
                        exclude_img: true,
                        exclude_links: true,
                        exclude_inputs: true,
                        preserveColors: false
                    });
                }
            });

        });
    </script>
