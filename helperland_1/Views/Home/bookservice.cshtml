            book.ServiceStartDate = $("#date").val();
            book.SubTotal = parseFloat($("#total_price_1").text());
            book.TotalCost =parseFloat($("#total_price_2").text());
            book.ServiceHours = parseFloat($("#total_time").text());
            book.HasPets = $("#forpets").is(":checked");
            console.log(book);
            $.ajax({

                url: '@Url.Action("savebooking", "home")',
                type: 'post',
                dataType: 'json',
                contentType: 'application/json',
                data: JSON.stringify(book),

                success: function (resp) {
                    $("#vraj").html(resp);
