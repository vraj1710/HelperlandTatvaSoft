        $("#completebooking").on('click', function () {

       
            var book = {};
            book.Comments = $("#comment").val();
            book.extraId = parseInt($("#cabinets").attr("target") + $("#fridge").attr("target") + $("#oven").attr("target") + $("#wash").attr("target") + $("#windows").attr("target"));
           
           // DateTime newDateTime = Convert.ToDateTime(one).Add(TimeSpan.Parse(two));
